{ #category : #accessing }
H7Message >> at: anIndex [ 

	^ segments at: anIndex 
]

{ #category : #accessing }
H7Message >> at: anIndex ifAbsent: aBlock [
^segments  at: anIndex ifAbsent: aBlock
]

{ #category : #testing }
H7Message >> isPCD [ ^self msh isPCD
]

{ #category : #accessing }
H7Message >> msh [
^segments at: 1
]

{ #category : #printing }
H7Message >> printOn: aStream [


	aStream << 'H7msg: '<< self msh msgType .
]

{ #category : #accessing }
H7Message >> segments [

^	segments
]

{ #category : #accessing }
H7Message >> segs:arrayOfSegments [
segments :=arrayOfSegments 
]

